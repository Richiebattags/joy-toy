; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="JoyVibrator"
config/features=PackedStringArray("4.6", "Forward Plus")
boot_splash/bg_color=Color(0.06666667, 0.06666667, 0.06666667, 1)
boot_splash/image="uid://dgqv2cjya3fqe"
config/icon="uid://dgqv2cjya3fqe"
boot_splash/minimum_display_time=500

[display]

window/size/viewport_width=270
window/size/viewport_height=480
window/size/resizable=false
window/size/borderless=true
window/size/transparent=true
window/size/maximize_disabled=true
window/per_pixel_transparency/allowed=true

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[importer_defaults]

svg={
&"base_scale": 4.0
}

[physics]

3d/physics_engine="Jolt Physics"

[rendering]

rendering_device/driver.windows="d3d12"
viewport/transparent_background=true
